<Window x:Class="AtlantaRentals.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Atlanta Rentals Login" Height="238.061" Width="335.447">
    <Grid>
        <Label Content="User Name
               " HorizontalAlignment="Left" Margin="15,12,0,0" VerticalAlignment="Top"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="176,12,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Label Content="Password" HorizontalAlignment="Left" Margin="15,57,0,0" VerticalAlignment="Top"/>
        <Label Content="Login as" HorizontalAlignment="Left" Margin="15,102,0,0" VerticalAlignment="Top"/>
        <PasswordBox HorizontalAlignment="Left" Margin="176,60,0,0" VerticalAlignment="Top" Height="23" Width="120"/>
        <ComboBox x:Name="cbUserType" HorizontalAlignment="Left" Margin="176,106,0,0" VerticalAlignment="Top" Width="120">
            <ComboBoxItem Content="Manager"></ComboBoxItem>
            <ComboBoxItem Content="Staff"></ComboBoxItem>
            <ComboBoxItem Content="Property Owner"></ComboBoxItem>
            <ComboBoxItem Content="Client"></ComboBoxItem>
        </ComboBox>
        <Button x:Name="btnLogin" Content="Login" HorizontalAlignment="Left" Margin="221,174,0,0" VerticalAlignment="Top" Width="75" Click="btnLogin_Click"/>
    </Grid>
</Window>
